package com.loan;

public class Mortgage extends Loan {
	
	

	
	
	public Mortgage( String issuedBy,String type, int term) {
		super(issuedBy,type,term);
	}
	
	
	public double CalculateInterestOnLoanByTerm(int term) {
		double interestRate;
	    if(term <= 15) {
	    	 return interestRate = 0.09;
	    }  
	    else {
	    	return interestRate = 0.15;	
	    }
		
	}
	
	}
	
