package com.loan;

public class Mortgage extends Loan {
	
	final String TYPE = "Mortgage";
	
	private int term;
	
	public int getTerm() {
		return term;
	}
	public void setTerm(int term) {
		this.term = term;
	}
	
	
	public Mortgage(int baseRate, String issuedBy, int term) {
		super(baseRate, issuedBy);
		this.term = term;
	}
	
	

}
