<?xml version="1.0" encoding="UTF-8"?>
<configuration status ="info" monitorInterval="10">
<Appenders>
<Console name="STDERR" target="SYSTEM_ERR">
<PatternLayout pattern ="[%d{dd MMM yyyy HH:mm:ss,SSS}{GMT}] %t %-5p %c - %m%n" />
</Console>
</Appenders>
<Loggers>
<Root level ="trace">
<appender-ref ref ="STDERR" level = "trace"/>
</Root>
</Loggers>

</configuration>
